<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Dotykačka API Provider Template -->
        <record id="dotykacka_api_provider_template" model="api_manager.provider">
            <field name="name">Dotykačka API (Template)</field>
            <field name="internal_reference">dotykacka_template</field>
            <field name="server_domain">api.dotypos.com</field>
            <field name="server_scheme">https</field>
            <field name="authentication_method">bearer_token</field>
            <field name="dynamic_token" eval="True"/>
        </record>

    </data>
</odoo>
